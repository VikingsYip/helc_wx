      <view class="row row-between row_line bor-bom-5">
        <view class="row text-34">
            {{item.corporateName}}
        </view>
        <view class="row" style="margin-left:20rpx">
          <view class="btn-green-s" bindtap="clickToChange" data-id="{{item.id}}">修改</view>
        </view>
      </view>
   </block>
</view>

